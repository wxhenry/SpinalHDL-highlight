{
	"name": "spinalhdl-highlight",
  "displayName": "SpinalHDL highlight",
  "description": "This extension highlights SpinalHDL code",
  "license": "MIT",
  "repository": "wxhenry/SpinalHDL-highlight",
	"version": "0.0.1",
	"engines": {
    "vscode": "^1.93.0"
	},
	"categories": [
		"Programming Languages"
	],
  "contributes": {
    "languages": [{
      "id": "scala",
      "aliases": ["scala", "Scala"],
      "extensions": [".scala"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "scala",
      "scopeName": "source.scala",
      "path": "./Scala.tmLanguage"
    }]
	}
}